<section class="contact" id="contact">
  <h2 class="heading">Entre em Contato <span>Comigo!</span></h2>
  <div class="contact-box">
    <input type="submit" value="instagram" class="btn-instagram">
    <input type="submit" value="whatsapp" class="btn-whatsapp">
    <input type="submit" value="linkedin" class="btn-linkedin">
  </div>
  <h2 class="heading">Ou</h2>
  <form action="https://formsubmit.co/dev.eduardoazevedo@gmail.com" method="POST">
    <div class="input-box">
      <input type="text" placeholder="Nome completo" name="nome" required>
      <input type="email" placeholder="Email" name="email" required>
    </div>
    <div class="input-box">
      <input type="number" placeholder="Celular"  name="celular">
      <input type="text" placeholder="email assunto" name="assunto">
    </div>
    <textarea name="mensagem" cols="30" rows="10" placeholder="Sua mensagem" required></textarea>
    <input type="submit" value="Enviar Mensagem" class="btn">
  </form>
</section>
